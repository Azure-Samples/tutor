{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Avatar",
      "type": "debugpy",
      "cwd": "${workspaceFolder}\\src\\avatar",
      "request": "launch",
      "module": "uvicorn",
      "env": {
        "Path": "${env:Path};${workspaceFolder}\\src"
      },
      "args": ["app.main:app", "--reload", "--port", "8084"],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "Essay",
      "type": "debugpy",
      "cwd": "${workspaceFolder}\\src\\essays",
      "request": "launch",
      "module": "uvicorn",
      "env": {
        "Path": "${env:Path};${workspaceFolder}\\src"
      },
      "args": ["app.main:app", "--reload", "--port", "8083"],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "Questions",
      "type": "debugpy",
      "cwd": "${workspaceFolder}\\src\\questions",
      "request": "launch",
      "module": "uvicorn",
      "env": {
        "Path": "${env:Path};${workspaceFolder}\\src"
      },
      "args": ["app.main:app", "--reload", "--port", "8082"],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "Configuration",
      "type": "debugpy",
      "cwd": "${workspaceFolder}\\src\\configuration",
      "request": "launch",
      "module": "uvicorn",
      "env": {
        "Path": "${env:Path};${workspaceFolder}\\src"
      },
      "args": ["app.main:app", "--reload", "--port", "8081"],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "Python Debugger: Python File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}"
    }
  ]
}
